{
  "name": "scannable",
  "version": "0.3.0",
  "description": "A general purpose scannable creation library.",
  "homepage": "https://leodog896.github.io/scannable",
  "bugs": {
    "url": "https://github.com/LeoDog896/scannable/issues"
  },
  "author": {
    "name": "Tristan F.",
    "email": "leodog896@gmail.com",
    "url": "https://github.com/LeoDog896"
  },
  "contributors": [
    {
      "name": "Alasdair Mercer",
      "email": "mercer.alasdair@gmail.com",
      "url": "https://neocotic.com"
    }
  ],
  "license": "GPL-3.0",
  "keywords": [
    "qr",
    "code",
    "scannable"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=15"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build --format esm",
    "test": "tsdx test",
    "test:watch": "tsdx test --watch",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "noCJS": "rm -f ./dist/*.cjs.*",
    "prepublishOnly": "npm run build && npm run noCJS && npm run size && eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/scannable.esm.js",
  "size-limit": [
    {
      "path": "dist/scannable.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/jest": "^27.4.1",
    "@types/split": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.12.0",
    "eslint-config-notninja": "^0.4.0",
    "eslint-plugin-functional": "^4.2.0",
    "eslint-plugin-perf-standard": "^1.0.3",
    "eslint-plugin-tsdoc": "^0.2.14",
    "husky": "^7.0.4",
    "size-limit": "^7.0.8",
    "split": "^1.0.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "tsutils": "^3.21.0",
    "typescript": "^4.6.2"
  }
}
